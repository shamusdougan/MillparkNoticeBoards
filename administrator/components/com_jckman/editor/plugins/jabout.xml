<?xml version="1.0"?>
<install version="1.5" type="plugin" plugin="jabout" method="upgrade">
	<version>0.1</version>
	<creationDate>Feb 2012</creationDate>
	<author>CKSource.com</author>
	<authorUrl>http://cksource.com/</authorUrl>
	<license>GNU/GPLv2</license>
	<description>Core JAbout plugin</description>
	<languages>
	</languages>
	<params>
		<param name="dialogname"  type="hidden" default="about" />
		<param name="dialogtitle" type="jscombo" default="Default" label="Dialog Title" description="The Title shown in the dialog for this plugin" >
			<option value="Default">Default</option>
			<option value="Enter new title here">Enter new title here</option>
		</param>	
		<param name="height" type="text" default="315" label="Dialog height" description="The height of dialog" />
		<param name="width" type="text" default="390" label="Dialog width" description="The width of dialog"/>
	</params>	
</install>
